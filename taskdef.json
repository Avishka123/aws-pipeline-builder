{
  "family": "aws-pipeline-builder-task-definition",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "base.container",
      "image": "ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/cpcoludregistry:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "essential": true
    }
  ]
}
