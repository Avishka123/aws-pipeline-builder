{
  "family": "aws-pipeline-builder-task-definition",
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "arn:aws:iam::<your-account-id>:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "AwsPipelineBuilderContainer",
      "image": "<your-account-id>.dkr.ecr.us-east-1.amazonaws.com/cpcoludregistry:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "essential": true
    }
  ]
}